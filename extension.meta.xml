<?xml version="1.0" encoding="UTF-8"?>
<extension id="members_anti_brute_force" status="released" xmlns="http://symphony-cms.com/schemas/extension/1.0">
	<name>Members Anti Brute Force</name>
	<description lang="en">Companion to the Anti Brute Force extension. Adds tracking of Members login and password reset.</description>
	<repo type="github">https://github.com/michael-e/members_anti_brute_force</repo>
	<dependencies>
		<dependency version="1.4.6">anti_brute_force</dependency>
		<dependency version="1.4">members</dependency>
	</dependencies>
	<types>
		<type>Security</type>
		<type>Login</type>
		<type>Brute Force</type>
	</types>
	<authors>
		<author>
			<name github="michael-e" symphony="michael-e">Michael Eichelsdoerfer</name>
			<website>http://www.michael-eichelsdoerfer.de</website>
		</author>
	</authors>
	<releases>
		<release version="1.1.1" date="2014-08-20" min="2.4">
			- Fix compatibility info; newer Symphony versions should work.
		</release>
		<release version="1.1" date="2014-08-20" min="2.4" max="2.5">
			- Fix issue with colored lists. Now requires Anti Brute Force 1.4.6.
		</release>
		<release version="1.0" date="2014-08-09" min="2.4" max="2.5">
			- Initial release
		</release>
	</releases>
</extension>
